        <h2 class ="tit" >Slot</h2>

      <form class="f" action="/updates" method="post">
        <div class="form-group row">
          <label for="slot_id" class="col-4 col-form-label">Slot_id</label> 
          <div class="col-8">
            <input id="slot_id" name="slot_id" placeholder="enter slot_id" type="text" class="form-control" required="required">
